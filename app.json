{
  "expo": {
    "name": "CoffeeFocus",
    "slug": "CoffeeFocus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/garden/arvore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/garden/arvore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seunome.coffeefocus",
      "icon": "./assets/garden/arvore.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/garden/arvore.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.seunome.coffeefocus",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/garden/arvore.png"
    },
    "plugins": [
      "expo-font",
      "expo-audio"
    ]
  }
}